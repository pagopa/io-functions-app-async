COMPOSE_PROJECT_NAME=io-fn-template
SLOT_TASK_HUBNAME=IoFunctionsNotifications

FUNCTIONS_WORKER_RUNTIME=node
FUNCTIONS_V2_COMPATIBILITY_MODE=true

COSMOSDB_PORT=3000
API_GATEWAY_PORT=80

APPINSIGHTS_INSTRUMENTATIONKEY=IDoNotKnow
APPINSIGHTS_SAMPLING_PERCENTAGE=20


STORAGE_CONN_STRING=<Set Conn String>

AzureWebJobsStorage=${STORAGE_CONN_STRING}

RETRY_ATTEMPT_NUMBER=10

NOTIFICATIONS_QUEUE_NAME=push-notifications
NOTIFICATIONS_STORAGE_CONNECTION_STRING=${STORAGE_CONN_STRING}


# ------------------------------------
# Variable used during transition to new NH management
# ------------------------------------

# Possible values : "none" | "all" | "beta" | "canary"
NH_PARTITION_FEATURE_FLAG=none

BETA_USERS_STORAGE_CONNECTION_STRING=${STORAGE_CONN_STRING}
BETA_USERS_TABLE_NAME=nhpartitiontestusers
CANARY_USERS_REGEX=^([(0-9)|(a-f)]{63}0)|([(0-9)|(a-f)]{62}[(0-7)]{1}1)$


# ------------------------------------
# Notification Hub Management Variables
# ------------------------------------

# Legacy Notification Hub variables
AZURE_NH_HUB_NAME=io-notification-hub-mock
AZURE_NH_ENDPOINT=Endpoint=sb://host.docker.internal:30000;SharedAccessKeyName=DefaultFullSharedAccessSignature;SharedAccessKey=foobar

"AZURE_NOTIFICATION_HUB_PARTITIONS": [{ "partitionRegex": "^[0-3]", "envVariablePrefix": "AZURE_NH_PARTITION_1"}, { "partitionRegex": "^[4-7]", "envVariablePrefix": "AZURE_NH_PARTITION_2"}, { "partitionRegex": "^[8-b]", "envVariablePrefix": "AZURE_NH_PARTITION_3"}, { "partitionRegex": "^[c-f]", "envVariablePrefix": "AZURE_NH_PARTITION_4"}]

AZURE_NH_PARTITION_1_HUB_NAME=io-notification-hub-mock-1
AZURE_NH_PARTITION_1_ENDPOINT=Endpoint=sb://host.docker.internal:30000;SharedAccessKeyName=DefaultFullSharedAccessSignature;SharedAccessKey=foobar

AZURE_NH_PARTITION_2_HUB_NAME=io-notification-hub-mock-2
AZURE_NH_PARTITION_2_ENDPOINT=Endpoint=sb://host.docker.internal:30000;SharedAccessKeyName=DefaultFullSharedAccessSignature;SharedAccessKey=foobar

AZURE_NH_PARTITION_3_HUB_NAME=io-notification-hub-mock-3
AZURE_NH_PARTITION_3_ENDPOINT=Endpoint=sb://host.docker.internal:30000;SharedAccessKeyName=DefaultFullSharedAccessSignature;SharedAccessKey=foobar

AZURE_NH_PARTITION_4_HUB_NAME=io-notification-hub-mock-4
AZURE_NH_PARTITION_4_ENDPOINT=Endpoint=sb://host.docker.internal:30000;SharedAccessKeyName=DefaultFullSharedAccessSignature;SharedAccessKey=foobar


# ------------------------------------
# Variables useful for testing ad mocking
# ------------------------------------
# needed to connect to cosmosdb server
NODE_TLS_REJECT_UNAUTHORIZED=0
# needed to connect to mock notification hub
AZURE_ENABLE_STRICT_SSL=false